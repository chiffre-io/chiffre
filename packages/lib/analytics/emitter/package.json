{
  "name": "@chiffre/analytics-emitter",
  "version": "0.0.1",
  "main": "dist/emitter.js",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:bundle build:copy",
    "build:bundle": "parcel build ./src/index.ts --out-file analytics.js --detailed-report 20 --experimental-scope-hoisting",
    "build:copy": "cp -f ./dist/analytics.js ../../../services/api/public/embed/",
    "clean": "rm -rf dist .cache node_modules"
  },
  "dependencies": {
    "@chiffre/analytics-core": "^0.0.1",
    "@chiffre/crypto-box": "^0.0.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  },
  "browser": {
    "crypto": false,
    "buffer": false
  }
}
