{
  "name": "@chiffre/analytics-emitter",
  "version": "0.0.1",
  "main": "dist/emitter.js",
  "types": "lib/types.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:tsc build:bundle build:cors",
    "build:tsc": "tsc",
    "build:bundle": "parcel build ./src/index.ts --out-file analytics.js --detailed-report 20 --experimental-scope-hoisting",
    "build:cors": "echo '*' > ./dist/CORS",
    "clean": "rm -rf dist .cache node_modules",
    "deploy": "surge -p ./dist -d https://embed.chiffre.io"
  },
  "dependencies": {
    "@chiffre/analytics-core": "^0.0.1",
    "@chiffre/crypto-box": "^0.0.1"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  },
  "browser": {
    "crypto": false,
    "buffer": false
  }
}
