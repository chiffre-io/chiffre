{
  "name": "@chiffre/web",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next",
    "ci": "run-s build",
    "build": "next build",
    "build:emitter": "parcel build -d ./public -o emitter.js ./src/emitter/index.ts",
    "start": "NODE_ENV=production yarn tsnode ./src/server/server.ts",
    "clean": "rm -rf ./out ./.next",
    "export": "next export",
    "deploy": "surge -p ./out -d https://chiffre.io",
    "release": "yarn clean && yarn build && yarn export && yarn deploy",
    "demo:serve": "serve ./demo",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@47ng/check-env": "^2.0.0",
    "@chakra-ui/core": "^0.5.2",
    "@chiffre/api-types": "^0.0.1",
    "@chiffre/client": "^0.0.1",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@zeit/next-source-maps": "^0.0.3",
    "emotion-theming": "^10.0.27",
    "formik": "2.1.2",
    "gravatar-url": "^3.1.0",
    "hibp": "^8.0.0",
    "http-status-codes": "^1.4.0",
    "next": "^9.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-qr-svg": "^2.2.1",
    "react-spaces": "^0.1.23",
    "react-table": "7.0.0-rc.15",
    "react-use": "^13.21.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "timeago.js": "^4.0.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.2.0",
    "@types/node": "^13.1.8",
    "@types/react-table": "^7.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/zxcvbn": "^4.4.0",
    "dotenv": "^8.2.0",
    "parcel-bundler": "^1.12.4",
    "serve": "^11.3.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "node"
  }
}
